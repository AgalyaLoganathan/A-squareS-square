<h3>Java Quiz Question 2</h3>
<form>
  <div class="form-group">
    <label for="question_title"><%= details.question_title %></label>
    <p class="question_block"><%= details.question_text%></p>
  </div>
  <% for(var i = 0; i < details.question_options.length; i++) { %>
    <div class="checkbox">
    <label>
      <input type="checkbox"> <%= details.question_options[i] %>
    </label>
  </div>
  <% } %>
  <button type="submit" class="btn btn-default">Submit</button>
</form>
</br>
<div class="progress">
 <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="50" style="width: "+ <%= progress%> >
  <%= progress+"%" %>
</div>
</div>
